{
    "name": "silbinarywolf/silverstripe-phpstan",
    "description": "PHPStan for Silverstripe",
    "type": "library",
    "keywords": [
        "php",
        "silverstripe",
        "static",
        "analysis",
        "phpstan",
        "scrutinizer"
    ],
    "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Jake Bentvelzen",
        "email": "jake@symbiote.com.au"
    }],
    "require": {
        "php": "~7.0",
        "phpstan/phpstan": "~0.9.0",
        "silverstripe/framework": "~3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": { 
            "SilbinaryWolf\\SilverstripePHPStan\\": [
                "src/",
                "src/type",
                "src/reflection"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}